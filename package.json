{
  "name": "bultclient",
  "version": "1.0.0-beta",
  "description": "The Bult Client",
  "private": true,
  "main": "dist/App.js",
  "bin": "dist/App.js",
  "typings": "dist/App",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "clean": "rm -rf ./dist/*",
    "full-start": "npm run bult-prepare && npm run start",
    "start": "node ./dist/App",
    "grunt": "grunt",
    "compile": "tsc -p .",
    "bult-prepare": "npm run grunt && npm run compile",
    "package": "npm run bult-prepare && pkg package.json --out-path dist/release",
    "release": "clear && echo 'Preparing Bult for published release.' && rm -rf ./node_modules && npm install --production && npm run clean && npm run compile && pkg package.json --out-path dist/release"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BultApp/Client.git"
  },
  "author": "MarkedBots",
  "dependencies": {
    "axios": "^0.18.0",
    "bcryptjs": "^2.4.3",
    "better-console": "^1.0.1",
    "body-parser": "^1.18.3",
    "dotenv": "^6.0.0",
    "execa": "^0.10.0",
    "express": "^4.16.3",
    "express-session": "^1.15.6",
    "file-exists": "^5.0.1",
    "ip": "^1.1.5",
    "lockfile": "^1.0.4",
    "lowdb": "^1.0.0",
    "mkdirp": "^0.5.1",
    "multer": "^1.3.0",
    "public-ip": "^2.4.0",
    "pug": "^2.0.3",
    "request": "^2.87.0",
    "rimraf": "^2.6.2",
    "to-time": "^1.0.2",
    "touch": "^3.1.0",
    "validator": "^10.4.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.1",
    "@types/dotenv": "^4.0.3",
    "@types/execa": "^0.9.0",
    "@types/express": "^4.16.0",
    "@types/express-session": "^1.15.10",
    "@types/lockfile": "^1.0.0",
    "@types/lowdb": "^1.0.5",
    "@types/mkdirp": "^0.5.2",
    "@types/multer": "^1.3.7",
    "@types/request": "^2.47.1",
    "@types/rimraf": "^2.0.2",
    "@types/touch": "^3.1.0",
    "@types/validator": "^9.4.1",
    "grunt": "^1.0.3",
    "grunt-cli": "^1.2.0",
    "grunt-contrib-watch": "^1.1.0",
    "grunt-ts": "^6.0.0-beta.20",
    "grunt-tslint": "^5.0.2",
    "pkg": "^4.3.3",
    "tslint": "^5.11.0",
    "typescript": "^2.9.2"
  },
  "pkg": {
    "assets": [
      "views/**/*"
    ]
  }
}
